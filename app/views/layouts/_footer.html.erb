     Signed in as <%= current_user.full_name %> (<%= link_to "Log out", session_path("current"), method: :delete %>)(<%= link_to "Admin User Management", admin_users_path %>)
     <% else %>
      <%= link_to "Log In", new_session_path %> | <%= link_to "Sign Up", new_user_path %>
